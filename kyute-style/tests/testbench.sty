

colors: {
    background        : color(#554411);
    background-alt    : color(#554877);
    background2       : $background;
}



borders: {
    colors : [ColorSet] {
        background : color(#554411);
    }

    button-border     : border(1px inside $background) ;
    button-border2    : border(1px #444)               ;
    button-background : linear-gradient(90deg, #000, #FFF, #DDD 90% 90%);
}

